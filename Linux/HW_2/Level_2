curl --url https://en.wikipedia.org/wiki/Harry_Potter | grep -c "Harry" | awk '{print "Harry:"  $1 "["}' | tr '\n' ' '> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | sed -n '/Harry/=' | tr '\n' ', ' | awk '{print $0 "]"}'>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | grep -c "Hogwarts" | awk '{print "Hogwarts:"  $1 "["}' | tr '\n' ' '>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | sed -n '/Hogwarts/=' | tr '\n' ', ' | awk '{print $0 "]"}'>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | grep -c "Lord" | awk '{print "Lord:"  $1 "["}' | tr '\n' ' '>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | sed -n '/Lord/=' | tr '\n' ', ' | awk '{print $0 "]"}'>> file
