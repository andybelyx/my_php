curl --url https://en.wikipedia.org/wiki/Harry_Potter | grep -o "Harry" | wc -l | awk '{print "Harry:"  $1 "["}' | tr '\n' ' '> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | sed -n '/Harry/=' | tr '\n' ', ' | awk '{print $0 "]"}'>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | grep -o "Hogwarts" | wc -l | awk '{print "Hogwarts:"  $1 "["}' | tr '\n' ' '>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | sed -n '/Hogwarts/=' | tr '\n' ', ' | awk '{print $0 "]"}'>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | grep -o "Lord" | wc -l | awk '{print "Lord:"  $1 "["}' | tr '\n' ' '>> file && curl --url https://en.wikipedia.org/wiki/Harry_Potter | sed -n '/Lord/=' | tr '\n' ', ' | awk '{print $0 "]"}'>> file
